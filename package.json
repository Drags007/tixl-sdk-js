{
  "name": "@tixl/tixl-sdk-js",
  "version": "0.1.7",
  "scripts": {
    "prepublish": "yarn build",
    "build": "yarn build:js && yarn build:package && yarn build:worker && yarn build:lib",
    "build:js": "tsc",
    "build:package": "cp src/_package.json dist/package.json && touch dist/.npmignore && cp .yarnrc dist/",
    "build:worker": "npx webpack --config web-worker/webpack.config.js",
    "build:lib": "cp lib/ntru/dist/ntru.js dist/ntru.js && cp lib/pedersen/dist/pedersen.js dist/secp256k1.js && cp lib/pedersen/prebuilt/libsecp256k1.js dist/libsecp256k1.js"
  },
  "files": [
    "dist/package.json",
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@tixl/tixl-ntru": "0.1.11",
    "@tixl/tixl-pedersen-commitments": "2.1.1",
    "@tixl/tixl-types": "0.6.7",
    "aes-js": "3.1.2",
    "axios": "0.19.2",
    "base64-js": "1.3.1",
    "bs58": "4.0.1",
    "buffer": "5.4.3",
    "build-query-string": "1.2.1",
    "hash-emoji": "0.0.4",
    "js-sha256": "0.9.0",
    "jsbi": "3.1.1",
    "lodash": "4.17.19",
    "randombytes": "2.1.0",
    "react-redux": "7.2.1",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "secp256k1": "3.7.1"
  },
  "devDependencies": {
    "@types/aes-js": "3.1.0",
    "@types/base64-js": "1.2.5",
    "@types/bs58": "4.0.1",
    "@types/lodash": "4.14.158",
    "@types/node": "13.7.0",
    "@types/randombytes": "2.0.0",
    "@types/react": "16.9.43",
    "@types/react-redux": "7.1.9",
    "@types/secp256k1": "3.5.0",
    "prettier": "2.0.5",
    "ts-loader": "8.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.7.5",
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12"
  }
}
