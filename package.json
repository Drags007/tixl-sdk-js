  },
  "dependencies": {
    "@tixl/tixl-types": "0.6.7",
    "aes-js": "3.1.2",
    "base64-js": "1.3.1",
    "bs58": "4.0.1",
    "buffer": "5.4.3",
    "js-sha256": "0.9.0",
    "jsbi": "3.1.1",
    "lodash": "4.17.19",
    "randombytes": "2.1.0",
    "secp256k1": "3.7.1"
  },
  "devDependencies": {
    "@types/aes-js": "3.1.0",
    "@types/base64-js": "1.2.5",
    "@types/bs58": "4.0.1",
    "@types/lodash": "4.14.158",
    "@types/node": "13.7.0",
    "@types/randombytes": "2.0.0",
    "@types/secp256k1": "3.5.0",
    "prettier": "2.0.5",
    "ts-loader": "8.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.7",
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12"
  }
